{
  "name": "@ipincamp/web-scraper",
  "version": "1.0.15",
  "description": "Simple and lightweight web scraper. Works well in 2023!",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.18.2",
    "npm": ">=9.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipincamp/web-scraper.git"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "typescript"
  ],
  "author": "ipincamp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ipincamp/web-scraper/issues"
  },
  "homepage": "https://github.com/ipincamp/web-scraper#readme",
  "dependencies": {
    "axios": "^1.6.1",
    "cheerio": "^1.0.0-rc.12",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
