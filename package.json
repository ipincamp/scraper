{
  "name": "@ipincamp/scraper",
  "version": "1.0.13-rev",
  "description": "Web Scraper",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "test": "exit 0"
  },
  "publishConfig": {
    "@ipincamp:registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipincamp/scraper.git"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "scraper"
  ],
  "author": "ipincamp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ipincamp/scraper/issues"
  },
  "homepage": "https://github.com/ipincamp/scraper#readme",
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
